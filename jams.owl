<?xml version="1.0"?>
<rdf:RDF xmlns="http://w3id.org/polifonia/ontology/jams/"
     xml:base="http://w3id.org/polifonia/ontology/jams/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:mc="http://w3id.org/polifonia/ontology/musical-composition/"
     xmlns:mf="http://w3id.org/polifonia/ontology/musical-features/"
     xmlns:mp="http://w3id.org/polifonia/ontology/musical-performance/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:core="http://w3id.org/polifonia/ontology/core/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:chord="http://purl.org/ontology/chord/"
     xmlns:roman="http://w3id.org/polifonia/ontology/roman-chord/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:polifonia-res="http://w3id.org/polifonia/resource/">
    <owl:Ontology rdf:about="http://w3id.org/polifonia/ontology/jams/">
        <owl:versionIRI rdf:resource="http://w3id.org/polifonia/ontology/jams/0.1/"/>
        <dc:creator xml:lang="en">Andrea Poltronieri</dc:creator>
        <dc:creator xml:lang="en">Valentina Presutti</dc:creator>
        <dc:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <rdfs:comment xml:lang="en">An ontology for modelling a JAMS file containing different types of musical annotations. 
In particular, this ontology focuses on the Chord Annotations contained in the JAMS file and their attributes.</rdfs:comment>
        <rdfs:label xml:lang="en">JAMS Ontology</rdfs:label>
        <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/license"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/polifonia/ontology/core/hasID -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/core/hasID">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-composition/MusicalComposition"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/core/Identifier"/>
        <rdfs:label xml:lang="en">has ID</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasAnnotationType -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasAnnotationType">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotationTypeOf"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/AnnotationType"/>
        <rdfs:label xml:lang="en">Has Annotation Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasAnnotator -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasAnnotator">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasAnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdfs:comment xml:lang="en">Links a JAMS annotation to the Annotator the annotated the JAMS.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasAnnotatorType -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasAnnotatorType">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/AnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdfs:comment xml:lang="en">Links the Annotator class to its Annotator Type, describing how the JAMS annotations were generated.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasChordSubject -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasChordSubject">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/musical-features/Chord"/>
        <rdfs:label xml:lang="en">Has Chord Subject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasEndMusicTimeIndex -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasEndMusicTimeIndex">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/polifonia/ontology/jams/hasMusicTimeIndex"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeInterval"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeIndex"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasFormat -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasFormat">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/File"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/Format"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-features/Score"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdfs:comment xml:lang="en">Relates a Score or a Recording to the JAMS annotation describing the track.</rdfs:comment>
        <rdfs:label xml:lang="en">Has JAMS Annotation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasMusicTimeDuration -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasMusicTimeDuration">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeInterval"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeDuration"/>
        <rdfs:label xml:lang="en">Has Music Time Duration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasMusicTimeIndex -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasMusicTimeIndex">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeInterval"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeIndex"/>
        <rdfs:label xml:lang="en">Has Music Time Index</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasMusicTimeIndexComponent -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasMusicTimeIndexComponent">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeIndex"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeIndexComponent"/>
        <rdfs:label xml:lang="en">Has Music Time Index Component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasMusicTimeInterval -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasMusicTimeInterval">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isMusicTimeIntervalOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeInterval"/>
        <rdfs:label xml:lang="en">Has Music Time Interval</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasStartMusicTimeIndex -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasStartMusicTimeIndex">
        <rdfs:subPropertyOf rdf:resource="http://w3id.org/polifonia/ontology/jams/hasMusicTimeIndex"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeInterval"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeIndex"/>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasSubject -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasSubject">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/musical-features/MusicalFeature"/>
        <rdfs:label xml:lang="en">Has Subject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasValueType -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasValueType">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/MusicTimeDuration"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/MusicTimeIndexComponent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeValueType"/>
        <rdfs:label xml:lang="en">Has Value Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/includesObservation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/includesObservation">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdfs:comment xml:lang="en">Links a JAMS Annotation to the JAMS Onservation(s) it contains.</rdfs:comment>
        <rdfs:label xml:lang="en">Includes Observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isAnnotationTypeOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isAnnotationTypeOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/AnnotationType"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:label xml:lang="en">Is Annotation Type Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isAnnotatorOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotator. Links the Annotator to the JAMS Annotation annotated by such annotator.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/AnnotatorType"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotatorType. Links the Annotator Type to the Annotator class.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Type Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-features/Score"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasJAMSAnnotation. It links the JAMS Annotation to either a Recording o a Score.</rdfs:comment>
        <rdfs:label xml:lang="en">Is JAMS Annotation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isMusicTimeIntervalOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isMusicTimeIntervalOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/MusicTimeInterval"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">Is Music Time Interval Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isObservationOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isObservationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:comment xml:lang="en">Inverse of includesObservation. Links a JAMS Observation to the JAMS Annotation to which it belongs.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Observation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-composition/hasComposer -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-composition/hasComposer">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-composition/MusicalComposition"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:label xml:lang="en">Has Composer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-composition/hasScore -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-composition/hasScore">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-composition/MusicalComposition"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/musical-features/Score"/>
        <rdfs:label xml:lang="en">Has Score</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-performance/hasPerformerSituation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-performance/hasPerformerSituation">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/musical-performance/PerformerSituation"/>
        <rdfs:label xml:lang="en">Has Performer Situation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-performance/involvesPerformer -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-performance/involvesPerformer">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-performance/PerformerSituation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:label xml:lang="en">Involves Performer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-performance/performedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-performance/performedBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-features/Score"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:label xml:lang="en">Performed By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-performance/recordingOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-performance/recordingOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/musical-composition/MusicalComposition"/>
        <rdfs:label xml:lang="en">Recording Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#WasDerivedFrom -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#WasDerivedFrom">
