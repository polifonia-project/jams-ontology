    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/license"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/polifonia/ontology/core/hasID -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/core/hasID">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-composition/MusicalComposition"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/core/Identifier"/>
        <rdfs:label xml:lang="en">has ID</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/annotationValidFrom -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/annotationValidFrom">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSScoreAnnotation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MetricalTime"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasAnnotator"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasAnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isChordOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Relates a JAMS Score Annotation to its metrical information, expressed by the MetricalTime class.</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation Valid From</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasAnnotator -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasAnnotator">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasAnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isChordOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Links a JAMS annotation to the Annotator the annotated the JAMS.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasAnnotatorType -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasAnnotatorType">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/AnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isChordOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Links the Annotator class to its Annotator Type, describing how the JAMS annotations were generated.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/hasJAMSAnnotation">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-features/Score"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isChordOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Relates a Score or a Recording to the JAMS annotation describing the track.</rdfs:comment>
        <rdfs:label xml:lang="en">Has JAMS Annotation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/includesObservation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/includesObservation">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isChordOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Links a JAMS Annotation to the JAMS Onservation(s) it contains.</rdfs:comment>
        <rdfs:label xml:lang="en">Includes Observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isAnnotatorOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isAnnotatorOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isChordOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotator. Links the Annotator to the JAMS Annotation annotated by such annotator.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isAnnotatorTypeOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/AnnotatorType"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/Annotator"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isChordOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotatorType. Links the Annotator Type to the Annotator class.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Type Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isChordOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isChordOf">
        <owl:inverseOf rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/Chord"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/AudioChordOccurrence"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/ScoreChordOccurrence"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Inverse of ofChord. It describes the relation between a Chord and a JAMS Observation, which can be either Audio (AudioChordOccurrence) or Score (ScoreChordOccurrence).</rdfs:comment>
        <rdfs:label xml:lang="en">Is Chord Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isJAMSAnnotationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-features/Score"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Inverse of hasJAMSAnnotation. It links the JAMS Annotation to either a Recording o a Score.</rdfs:comment>
        <rdfs:label xml:lang="en">Is JAMS Annotation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isMetricalTimeOf">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/MetricalTime"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSScoreAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/isObservationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Inverse of startsAt. It relates the Metrical Time of a JAMS Score Observation, expressing the score observation&apos;s timing information.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Metrical Time Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/isObservationOf -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/isObservationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSObservation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://w3id.org/polifonia/ontology/jams/ofChord"/>
        <rdfs:comment xml:lang="en">Inverse of includesObservation. Links a JAMS Observation to the JAMS Annotation to which it belongs.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Observation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/jams/ofChord -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/jams/ofChord">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/AudioChordOccurrence"/>
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/jams/ScoreChordOccurrence"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/Chord"/>
        <rdfs:comment xml:lang="en">It describes the relationship between an observation and the agreement contained within it. The Observation can be either Audio or Score. The property thus links both AudioChordOccurrence and ScoreChordOccurrence to Chord.</rdfs:comment>
        <rdfs:label xml:lang="en">Of Chord</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-composition/hasComposer -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-composition/hasComposer">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-composition/MusicalComposition"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:label xml:lang="en">Has Composer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-composition/hasScore -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-composition/hasScore">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-composition/MusicalComposition"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/musical-features/Score"/>
        <rdfs:label xml:lang="en">Has Score</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-features/startsAt -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-features/startsAt">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/jams/JAMSScoreObservation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/jams/MetricalTime"/>
        <rdfs:label xml:lang="en">Starts At</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-performance/hasPerformerSituation -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-performance/hasPerformerSituation">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-performance/Recording"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/musical-performance/PerformerSituation"/>
        <rdfs:label xml:lang="en">Has Performer Situation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-performance/involvesPerformer -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-performance/involvesPerformer">
        <rdfs:domain rdf:resource="http://w3id.org/polifonia/ontology/musical-performance/PerformerSituation"/>
        <rdfs:range rdf:resource="http://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:label xml:lang="en">Involves Performer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://w3id.org/polifonia/ontology/musical-performance/performedBy -->

    <owl:ObjectProperty rdf:about="http://w3id.org/polifonia/ontology/musical-performance/performedBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/polifonia/ontology/musical-features/Score"/>
